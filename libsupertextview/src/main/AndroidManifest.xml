<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.geek.libsupertextview">
    <application
        android:allowBackup="true"
        android:supportsRtl="true">
        <activity android:name=".supertextview.SuperTextviewActivity"
            android:exported="true">
            <intent-filter>
                <action android:name="${applicationId}.hs.act.slbapp.MainActivity" />
                <category android:name="android.intent.category.DEFAULT" />
            </intent-filter>
        </activity>
        <activity android:name=".supertextview.TypeActivity" />
        <activity android:name=".supertextview.ListActivity"
            android:label="SuperTextView列表中使用" />
        <activity
            android:name=".supertextview.CommonTextViewActivity"
            android:label="CommonTextView" />
        <activity android:name=".supertextview.ClickActivity"
            android:label="SuperTextView点击事件"/>
        <activity
            android:name=".supertextview.SuperButtonActivity"
            android:label="SuperButton" />
        <activity android:name=".supertextview.ShapeActivity" />
        <activity android:name=".supertextview.ShadowActivity" />

    </application>

</manifest>